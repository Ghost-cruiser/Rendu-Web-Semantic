<div id="content-presenter" style="display: flex; padding:5vh; justify-content:center">
    <div class="card" style="width:50%;">
        <div class="card">
            <h2 style="text-align:center">Paramètres</h2>
        </div>
        <form method="post" class="card" style="padding:10px">
            <div class="card-block">
                <h3 class="card-title">Méthodes d'authentification</h3>
                <ul style="margin-bottom:3vh">
                    <% for(var key in data.parameters) { %>
                    <li>
                        <span>
                            <input type="checkbox" name="<%= key %>" <% if (data.parameters[key]){ %> checked <% } %> value="1" />
                        </span>
                        <span>
                            <%= key %>
                        </span>
                    </li>
                    <% } %>
                </ul>
                <input type="submit" value="Valider" class="btn btn-success form-control" />
            </div>
        </form>
    </div>
</div>